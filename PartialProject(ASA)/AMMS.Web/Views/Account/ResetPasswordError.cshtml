@model AMMS.Web.Models.ResetPasswordViewModel
@{
    ViewBag.Title = "ResetPasswordError";
}


<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">EmployeeId is not the same as it should be.</h2>

<div>
    <p>
        Please @Html.ActionLink("click here to reset password again", "ResetPassword", "Account", routeValues: new { code = "reset", employee = Model.EmployeeId }, htmlAttributes: new { id = "loginLink" })
    </p>
</div>
