@using ArmadaReports.Web.Repository
<div id="widget-panel-1" class="col-lg-5 col-md-10 col-sm-12 col-lg-offset-1 col-md-offset-1 non-pad-col">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h4 id="school-count-title" class="panel-title">EpiPen - Unique / Total Count of School</h4>
            <img src="~/Content/images/expand-icon.png" class="details-icon" onclick="epipenPopup.showSchoolCountPopup()">
        </div>
        <div class="panel-body epipen-panel-body-height">
            <div class="form-inline">
                <div class="form-group">
                    <select id="school-count-inpCustomerCategory" class="form-control form-control-height" title="select customer category">
                        <option value="All">All</option>
                        <option value="Disney">Disney</option>
                        <option value="Schools">Schools</option>
                    </select>
                </div>
                <div class="form-group">
                    <div class="input-group epipen-date date" data-provide="datepicker">
                        <input id="school-count-begin-date" type="text" class="form-control" title="select a begin date" value="@DateUtility.ThisMonth(DateTime.Today).Start.ToString("yyyy-MM-dd")">
                        <div class="input-group-addon">
                            <span class="glyphicon glyphicon-th"></span>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="input-group epipen-date date" data-provide="datepicker">
                        <input id="school-count-end-date" type="text" class="form-control" title="select a end date" value="@DateTime.Today.ToString("yyyy-MM-dd")">
                        <div class="input-group-addon">
                            <span class="glyphicon glyphicon-th"></span>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label class="radio-inline">
                        <input class="radio-inline-input" type="radio" name="screleasedate" id="school-count-by-state" value="" checked="checked"> By State
                    </label>
                </div>
                <div class="form-group">
                    <label class="radio-inline">
                        <input class="radio-inline-input" type="radio" name="screleasedate" id="school-count-by-date" value="ReleaseDate"> By Date
                    </label>
                </div>
                <div class="form-group">
                    <span class="fa fa-arrow-circle-o-right submit-icon" title="submit" onclick="utility.onSchoolCountSubmit()"></span>
                </div>
                <div class="form-group">
                    <span class="fa fa-download download-icon" title="download summary to an excel" onclick="epipenTableWrapper.downloadSchoolCountSummary()"></span>
                </div>
                <div class="form-group">
                    <span class="fa fa-download download-icon" title="download summary and details to an excel" onclick="epipenTableWrapper.downloadSchoolCountSummaryAndDetails()"></span>
                </div>
                <img id="school-count-loading" src="~/Content/images/loading_blue_lg.gif" class="loading-epipen" />
            </div>
            <div class="epipen-chart-container" id="school-count-table-div">
                <table id="school-count-table" class="table table-bordered table-hover epipen-table">
                    <thead>
                        <tr>
                            <td>School State</td>
                            <td>Total Unique School</td>
                            <td>Total School</td>
                            <td>Differences</td>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>

            <div class="btn-group-container">
                <div id="school-count-bg" class="btn-group" role="group" aria-label="">
                    <button type="button" data-date="today" class="btn btn-primary custom-btn-group btn-selected" onclick="utility.dateBtnClick('school-count-bg', this, epipenService.getSchoolCounts)">Today</button>
                    <button type="button" data-date="yesterday" class="btn btn-primary custom-btn-group" onclick="utility.dateBtnClick('school-count-bg', this, epipenService.getSchoolCounts)">Yesterday</button>
                    <button type="button" data-date="thisweek" class="btn btn-primary custom-btn-group" onclick="utility.dateBtnClick('school-count-bg', this, epipenService.getSchoolCounts)">This Week</button>
                    <button type="button" data-date="lastweek" class="btn btn-primary custom-btn-group" onclick="utility.dateBtnClick('school-count-bg', this, epipenService.getSchoolCounts)">Last Week</button>
                    <button type="button" data-date="currentmonth" class="btn btn-primary custom-btn-group" onclick="utility.dateBtnClick('school-count-bg', this, epipenService.getSchoolCounts)">Current Month</button>
                    <button type="button" data-date="lastmonth" class="btn btn-primary custom-btn-group" onclick="utility.dateBtnClick('school-count-bg', this, epipenService.getSchoolCounts)">Last Month</button>
                    <button type="button" data-date="ytd" class="btn btn-primary custom-btn-group" onclick="utility.dateBtnClick('school-count-bg', this, epipenService.getSchoolCounts)">YTD</button>
                    <button type="button" data-date="allorders" class="btn btn-primary custom-btn-group" onclick="utility.dateBtnClick('school-count-bg', this, epipenService.getSchoolCounts)">All Orders</button>
                </div>
            </div>
        </div>
    </div>
</div>

